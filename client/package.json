{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "react": "^16.9.0",
    "react-collapsible": "^2.6.0",
    "react-dom": "^16.9.0",
    "react-preloaders": "^3.0.3",
    "react-redux": "^7.1.1",
    "react-responsive-carousel": "^3.1.50",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-slick": "^0.25.2",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-saga": "^1.0.5",
    "slick-carousel": "^1.8.1",
    "stylelint": "^10.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-sass-js start-js",
    "start-js": "react-scripts start",
    "build": "npm run stylelint-build && npm run eslint && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stylelint-dev": "stylelint src/**/*.scss --fix --cache --cache-location .stylelintcache",
    "stylelint-build": "stylelint src/**/*.scss --fix",
    "eslint": "eslint src/**/*.js --fix --fix-type [layout]",
    "watch-sass-js": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:9000",
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "standard": "^14.1.0"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js"
      ],
      "extensions": "js"
    }
  }
}
